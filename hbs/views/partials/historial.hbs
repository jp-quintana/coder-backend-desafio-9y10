<h2 style="color:crimson">Historial</h2>
{{#if productos.[0]}}
  <div class="table-responsive">
    <table class="table table-dark">
      <tr style="color: yellow;">
        <th>Nombre</th>
        <th>Precio</th>
        <th>Foto URL</th>
      </tr>
      {{#each productos}}
      <tr>
        <td>{{this.nombre}}</td>
        <td>{{this.precio}}</td>
        <td>{{this.url}}</td>
      </tr>
      {{/each}}
    </table>
  </div>
{{else}}
  <h3 class="alert alert-danger">no se encontraron datos</h3>
{{/if}}
